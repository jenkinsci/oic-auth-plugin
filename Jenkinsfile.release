@Library(["ised-cicd-lib", "ised-cicd-lib-api"]) _

pipeline {
	agent {
       	label 'maven'
   	} 
     	
    options {
        disableConcurrentBuilds()
    }
  
   	environment {
		// GLobal Vars

		MVN_DEBUG_LOGGING = "false"
    }
  
    stages {
    	stage('build') {
			steps {
				script{
					release.releaseMavenLibrary()
				}
			}
    	}
    }
}